<body ng-app="workapp" ng-controller="index">
<div ng-view class="view-animate" style="height: 100%">

</div>

<!--底部导航-->
<nav class="mui-bar mui-bar-tab">
    <a class="mui-tab-item" ng-class="{'mui-active':active=='s'}"   ng-click="change('s')" href="#!/">
        <span class="mui-icon mui-icon-home"></span>
        <span class="mui-tab-label">首页</span>
    </a>
    <a class="mui-tab-item" href="#!/tong" ng-class="{'mui-active':active=='t'}"   ng-click="change('t')">
