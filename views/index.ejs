        <span class="mui-tab-label">通讯录</span>
    </a>
    <a class="mui-tab-item" href="#!/content" ng-class="{'mui-active':active=='x'}"   ng-click="change('x')">
        <span class="mui-icon mui-icon-compose"></span>
        <span class="mui-tab-label">写总结</span>
    </a>
    <a class="mui-tab-item" href="#!/set" ng-class="{'mui-active':active=='sz'}"   ng-click="change('sz')">
        <span class="mui-icon mui-icon-gear"></span>
        <span class="mui-tab-label">设置</span>
    </a>
</nav>
<input type="hidden" id="hiden" value="<%= userInfo %>" style="width: 100%;height: 100%;opacity: 0">
